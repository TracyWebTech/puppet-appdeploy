{
  "name": "tracywebtech-appdeploy",
  "version": "0.9.3",
  "author": "tracywebtech",
  "summary": "Puppet module to deplot applications",
  "license": "Apache License, Version 2.0",
  "source": "https://github.com/TracyWebTech/puppet-appdeploy",
  "project_page": "https://github.com/TracyWebTech/puppet-appdeploy",
  "issues_url": "https://github.com/TracyWebTech/puppet-appdeploy/issues",
  "description": "Puppet module to deploy applications",
  "dependencies": [
    {"name":"puppetlabs/apt","version_requirement":">= 1.0.0"},
    {"name":"puppetlabs/concat","version_requirement":">= 1.1.0"},
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.1.0"},
    {"name":"jfryman/nginx","version_requirement":">= 0.2.2"},
    {"name":"tracywebtech/pip","version_requirement":">= 1.3.2"},
    {"name":"tracywebtech/puias","version_requirement":">= 0.1.3"},
    {"name":"tracywebtech/openssl","version_requirement":">= 0.1.0"},
    {"name":"tracywebtech/supervisor","version_requirement":">= 1.1.6"}
  ],

  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease":[
        "6.0"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease":[
        "6.0",
        "7.0"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "12.04"
      ]
    }
  ]
}
